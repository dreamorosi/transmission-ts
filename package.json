{
	"name": "transmission-ts",
	"version": "0.0.3",
	"description": "An opinionated Transmission RPC client for Node.js written in TypeScript",
	"scripts": {
		"init-environment": "husky install",
		"build": "tsup",
		"lint": "biome lint .",
		"lint:fix": "biome check --write .",
		"test": "vitest --run --coverage",
		"docs:create": "typedoc --options typedoc.json"
	},
	"keywords": [
		"transmission",
		"api",
		"typescript",
		"rpc",
		"ts",
		"client"
	],
	"author": {
		"name": "Andrea Amorosi",
		"email": "dreamorosi@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dreamorosi/transmission-ts.git"
	},
	"bugs": {
		"url": "https://github.com/dreamorosi/transmission-ts/issues"
	},
	"homepage": "https://github.com/dreamorosi/transmission-ts#readme",
	"license": "MIT-0",
	"engines": {
		"node": ">=18"
	},
	"files": [
		"lib"
	],
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"lint-staged": {
		"*.{js,ts}": "biome check --write"
	},
	"tsup": {
		"entry": [
			"./src/**/*.ts"
		],
		"splitting": false,
		"sourcemap": true,
		"clean": true,
		"format": [
			"esm"
		],
		"minify": false,
		"dts": true,
		"outDir": "./lib"
	},
	"standard-version": {
		"header": "# Changelog\n\nAll notable changes to this project will be documented in this file.\n\n"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.4",
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^22.13.10",
		"@vitest/coverage-v8": "^3.0.8",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"standard-version": "^9.5.0",
		"tsup": "^8.4.0",
		"typedoc": "^0.24.8",
		"typedoc-plugin-markdown": "^3.15.3",
		"typedoc-plugin-missing-exports": "^2.0.0",
		"typescript": "~4.9.5",
		"vitest": "^3.0.8"
	},
	"dependencies": {
		"undici": "^7.5.0",
		"zod": "^3.24.2"
	}
}
