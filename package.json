{
  "name": "transmission-ts",
  "version": "0.0.1",
  "description": "An opinionated Transmission RPC client for Node.js written in TypeScript",
  "scripts": {
    "init-environment": "husky install",
    "build": "tsc",
    "lint": "eslint --ext .ts --no-error-on-unmatched-pattern src tests",
    "lint-fix": "eslint --fix --ext .ts --no-error-on-unmatched-pattern src tests",
    "test": "vitest --run --coverage"
  },
  "keywords": [
    "transmission",
    "api",
    "typescript",
    "rpc",
    "ts",
    "client"
  ],
  "author": {
    "name": "Andrea Amorosi",
    "email": "dreamorosi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreamorosi/transmission-ts.git"
  },
  "bugs": {
    "url": "https://github.com/dreamorosi/transmission-ts/issues"
  },
  "homepage": "https://github.com/dreamorosi/transmission-ts#readme",
  "license": "MIT-0",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.ts": "npm run lint-fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitest/coverage-c8": "^0.30.1",
    "esbuild": "^0.17.16",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.7",
    "typescript": "~4.9.5",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "undici": "^5.21.2",
    "zod": "^3.21.4"
  }
}
